<TabItem x:Class="RapportAgentPlugin.Views.PhysicalSpace.PhysicalSpaceTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RapportAgentPlugin.Views.PhysicalSpace"
             xmlns:physicalSpace="clr-namespace:RapportAgentPlugin.Views.PhysicalSpace"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <TabItem.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../SharedResources/Icons.xaml"/>
                <ResourceDictionary Source="../SharedResources/Other.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </TabItem.Resources>
    <TabItem.Header>
        <StackPanel Orientation="Horizontal">
            <Rectangle Width="12" Height="12" Margin="0,0,5,0" Fill="{StaticResource PhysicalSpaceIcon}"/>
            <TextBlock Text="Physical Space" />
        </StackPanel>
    </TabItem.Header>
</TabItem>
